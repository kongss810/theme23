    }
    .UserManual2_leftBox.active .animated-plus:before {
        transform: translate(-50%, -50%) rotate(90deg);
    }
    .UserManual2_leftBox.active .animated-plus:after {
        transform: translate(-50%,-50%) rotate(90deg);
