        .off('click')
        .on('click', function () {
          const $box = $(this).closest('.UserManual2_leftBox');
          const $list = $box.find('.UserManual2_leftList');

          if ($box.hasClass('active')) {
            $box.removeClass('active');
            $list.slideUp();
