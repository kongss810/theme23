</style>
<div class="page_UserManual2All color-{{ section.settings.color_scheme }} gradient">
  <div class="page-width">
    <div class="page_UserManual2_box {% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
      <div class="page_UserManual2_top">
        {%- if section.settings.title != blank -%}
          <h2 class="page_UserManual2_boxTitle my_h_48 font2 my_m_24">
            {{ section.settings.title }}
          </h2>
        {%- endif -%}
        {%- if section.settings.show_search != blank -%}
          <div class="page_UserManual2_Search section_button2">
            <input
              type="text"
              placeholder="{{ 'general.search.search2' | t }}"
              name=""
              id="searchInput"
              class="my_h_16"
            >
            <div class="sectionBrandCollection3_button my_h_16 my_m_14 font2 section_button2">
              {{ 'general.search.search' | t }}
              <span class="corner-tl"></span>
              <span class="corner-br"></span>
            </div>
            <span class="corner-tl"></span>
            <span class="corner-br"></span>
          </div>
        {%- endif -%}
      </div>
      <div class="line"></div>
      <div class="page_UserManual2_bottom">
        {% assign leftDatas = page.metafields.custom.user_manual.value.left_datas %}
        <div class="page_UserManual2_left">
          <!-- ‰Ω†ÁöÑ leftBox Âæ™ÁéØ -->
          <div class="page_UserManual2_noEmpty my_h_16 my_m_14" style="display: none;">
            {{ 'general.search.search_empty' | t }}
          </div>
          <div id="paginationContainer" class="pagination"></div>
        </div>

        <div class="line2"></div>
        <div class="page_UserManual2_right">
          {% if section.settings.right_title != blank %}
            <h3 class="page_UserManual2_rightTitle my_h_24 my_m_24 font2">
              <svg xmlns="http://www.w3.org/2000/svg" width="6" height="50" viewBox="0 0 6 50" fill="none">
                <path d="M6 45L0 50V5L6 0V45Z" fill="#DAB736"/>
              </svg>
              {{ section.settings.right_title }}
            </h3>
          {% endif %}
          <div class="page_UserManual2_rightList">
            {% assign rightDatas = page.metafields.custom.user_manual.value.right_data %}
            {% for article in rightDatas.value.more_topic_you_may_interested.value %}
              <div class="page_UserManual2_rightListBox my_h_16 my_m_14">
                <a href="{{ article.url }}">
                  {{ article.title }}
                </a>
              </div>
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<script id="userManualData" type="application/json">
  [
    {% for item in leftDatas.value %}
      {
        "product_list_title": "{{ item.product_list_title }}",
        "product_list_faqs": [
          {% for faq in item.product_list_faqs.value %}
            {
              "title": "{{ faq.title }}",
              "link": "{{ faq.link }}"
            }{% if forloop.last == false %},{% endif %}
          {% endfor %}
        ]
      }{% if forloop.last == false %},{% endif %}
    {% endfor %}
  ]
</script>

<script>
  $(function () {
    const rawData = $('#userManualData').text();
    const data = JSON.parse(rawData);

    const $root = $('.page_UserManual2_left'); // ÂÆπÂô®
    const $paginationContainer = $('#paginationContainer'); // ÂàÜÈ°µ
    const $searchInput = $('#searchInput');

    const perPage = `{{ section.settings.data_per_page  }}`;
    let filteredData = [...data];
    let currentPage = 1;

    function toggleEmptyState() {
        if (filteredData.length === 0) {
            $('.page_UserManual2_noEmpty').show();
            $('#paginationContainer').hide();
        } else {
            $('.page_UserManual2_noEmpty').hide();
            $('#paginationContainer').show();
        }
    }

    function renderHTML(items) {
      // Ê∏ÖÁ©∫ÊóßËäÇÁÇπÔºå‰øùÁïôÂàÜÈ°µÂÆπÂô®
      $root.find('.UserManual2_leftBox').remove();

      items.forEach((item, index) => {
        const $box = $(`
        <div class="UserManual2_leftBox">
          <div class="UserManual2_leftTitle my_h_24 my_m_16 font2">
            ${item.product_list_title}
            <span class="animated-plus"></span>
          </div>
          <div class="UserManual2_leftList">
            ${(item.product_list_faqs || [])
              .map((faq) => {
                if (!faq.title) return '';
                return `
                <div class="UserManual2_leftListSmall">
                  <div class="UserManual2_leftListSmall_title my_h_16 my_m_14">
                    <div class="UserManual2_leftListSmall_titleLeft">
                      <span>
                        <svg xmlns="http://www.w3.org/2000/svg" width="10" height="1" viewBox="0 0 10 1" fill="none">
                          <path d="M0 0.5H10" stroke="white"/>
                        </svg>
                      </span>
                      ${faq.title}
                    </div>
                    <a href="${faq.link}" target="_blank" class="UserManual2_leftListSmall_text">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16" fill="none">
                            <g clip-path="url(#clip0_1441_3537)">
                                <path d="M0 14H16V16H0V14ZM8 13L13 8H10V0H6V8H3L8 13Z" fill="white"/>
                            </g>
                            <defs>
                                <clipPath id="clip0_1441_3537">
                                <rect width="16" height="16" fill="white"/>
                                </clipPath>
                            </defs>
                        </svg>
                    </a>
                  </div>
                </div>
              `;
              })
              .join('')}
          </div>
        </div>
      `); 

        // ÈªòËÆ§Â±ïÂºÄÁ¨¨‰∏ÄÈ°µÁ¨¨‰∏Ä‰∏™
        if (currentPage === 1 && index === 0) {
            {% if section.settings.show_first %}
                $box.addClass('active');
                $box.find('.UserManual2_leftList').show();
            {% else %}
                $box.find('.UserManual2_leftList').hide();
            {% endif %}
        } else {
            $box.find('.UserManual2_leftList').hide();
        }

        $box.insertBefore($paginationContainer);
      });

      bindToggle(); // ÊØèÊ¨°Ê∏≤ÊüìÂêéÁªëÂÆöÊäòÂè†‰∫ã‰ª∂
    }

    function render(page = 1) {
      currentPage = page;

        toggleEmptyState(); // üëà ÂÖàÂà§Êñ≠ÊòØÂê¶‰∏∫Á©∫
        if (filteredData.length === 0) {
            // Ê∏ÖÁ©∫ÂàóË°®
            $('.UserManual2_leftBox').remove();
            return;
        }
      const start = (page - 1) * perPage;
      const end = start + perPage;
      renderHTML(filteredData.slice(start, end));
      renderPagination();

    }

    function renderPagination() {
      $paginationContainer.empty();
      const totalPages = Math.ceil(filteredData.length / perPage);
      if (totalPages <= 1) return;

      const $prev = $(`
        <div class="left-btn">
            <div class="section_button2">
                <svg xmlns="http://www.w3.org/2000/svg" width="10" height="18" viewBox="0 0 10 18" fill="none">
                <path d="M8.625 17.125L9.875 15.875L2.50001 8.56249L9.875 1.25L8.625 0L7.62939e-06 8.56249L8.625 17.125Z" fill="white"/>
                </svg>
                <span class="corner-tl"></span>
                <span class="corner-br"></span>
            </div>
        </div>
    `)
        .prop('disabled', currentPage === 1)
        .click(() => {
            render(currentPage - 1);
            $('html, body').animate({
                scrollTop: $('.page_UserManual2All').offset().top
            }, 300);
        });
      if (currentPage === 1) {
        $prev.addClass('no_button'); // Ââç‰∫î‰∏çÂèØÁÇπÂáªÂä† no_button
      }
      $paginationContainer.append($prev);

      // Ê∏ÖÁ©∫ÊóßÁöÑÈ°µÁ†Å
      $paginationContainer.find('.pagination_boxItem').remove();

      // ÂàõÂª∫ÂÆπÂô®
      const $box = $('<div class="pagination_boxItem my_h_20 my_m_16 font2"></div>');

      // ÁîüÊàêÈ°µÁ†Å
      let start = Math.max(1, currentPage - 2);
      let end = Math.min(totalPages, currentPage + 2);

      // Â¶ÇÊûúÁ¨¨‰∏ÄÈ°µ‰∏çÂú®ÊòæÁ§∫ËåÉÂõ¥ÔºåÊòæÁ§∫Á¨¨‰∏ÄÈ°µ
     if (start > 1) {
        $box.append($('<div></div>').text(1).click(() => {
        render(1);
        $('html, body').animate({scrollTop: $('.page_UserManual2All').offset().top}, 300);
        }));
        if (start > 2) $box.append('<div class="ellipsis">...</div>');
    }

    for (let i = start; i <= end; i++) {
        const $btn = $('<div></div>').text(i).click(() => {
        render(i);
        $('html, body').animate({scrollTop: $('.page_UserManual2All').offset().top}, 300);
        });
        if (i === currentPage) $btn.addClass('active');
        $box.append($btn);
    }

    if (end < totalPages) {
        if (end < totalPages - 1) $box.append('<div class="ellipsis">...</div>');
        $box.append($('<div></div>').text(totalPages).click(() => {
        render(totalPages);
        $('html, body').animate({scrollTop: $('.page_UserManual2All').offset().top}, 300);
        }));
    }


      // ÊèíÂÖ•Âà∞ÂàÜÈ°µÂÆπÂô®‰∏≠ÔºåÊîæÂú®Â∑≤ÊúâÁöÑÂ∑¶Âè≥ÊåâÈíÆ‰πãÈó¥
      // ÂÅáËÆæ $paginationContainer Â∑≤ÁªèÊúâÂ∑¶Âè≥ÊåâÈíÆÂ≠òÂú®
      $paginationContainer.find('.left-btn').after($box);

      const $next = $(`
     <div class="right-btn">
        <div class="section_button2">
            <svg xmlns="http://www.w3.org/2000/svg" width="10" height="18" viewBox="0 0 10 18" fill="none">
                <path d="M1.25 17.125L0 15.875L7.37499 8.56249L0 1.25L1.25 0L9.87499 8.56249L1.25 17.125Z" fill="white"/>
            </svg>
            <span class="corner-tl"></span>
            <span class="corner-br"></span>
        </div>
        </div>
    `)
        .prop('disabled', currentPage === totalPages)
        .click(() => {
        render(currentPage + 1);
            $('html, body').animate({scrollTop: $('.page_UserManual2All').offset().top}, 300);
        });
      if (currentPage === totalPages) {
        $next.addClass('no_button'); // Âêé‰∫î‰∏çÂèØÁÇπÂáªÂä† no_button
      }
      $paginationContainer.append($next);
    }

    // ÊêúÁ¥¢
    $('.sectionBrandCollection3_button').on('click', function () {
      const keyword = $('#searchInput').val().trim().toLowerCase();
      filteredData = keyword
        ? data.filter((item) => {
            // ÂåπÈÖçÂàÜÁ±ªÊ†áÈ¢ò
            if (item.product_list_title.toLowerCase().includes(keyword)) return true;

            // ÂåπÈÖçÊØè‰∏™ FAQ Ê†áÈ¢ò
            return (item.product_list_faqs || []).some((faq) => faq.title && faq.title.toLowerCase().includes(keyword));
          })
        : [...data];

      render(1); // ÊâßË°åÊ∏≤Êüì
    });

