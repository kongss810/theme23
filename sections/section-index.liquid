<style>
    #shopify-section-{{section.id}} .slideshow_all{
        margin-top: {{section.settings.Desktop_top}}px;
        margin-bottom: {{section.settings.Desktop_bottom}}px;

    }
    @media(max-width:999px) {
        #shopify-section-{{section.id}} .slideshow_all{
            margin-top: {{section.settings.Mobile_top}}px;
            margin-bottom: {{section.settings.Mobile_bottom}}px;
        }
    }
  {%- for block in section.blocks -%}
      {%- assign text_position = block.settings.text_position | split: '_' | first -%}


      {%- case text_position -%}
      {%- when 'top' -%}
          {%- assign section_items_alignment = 'flex-start' -%}
      {%- when 'middle' -%}
          {%- assign section_items_alignment = 'center' -%}
      {%- when 'bottom' -%}
          {%- assign section_items_alignment = 'flex-end' -%}
      {%- endcase -%}

      {%- case text_positionMobile -%}
        {%- when 'top' -%}
            {%- assign section_items_alignmentMobile = 'flex-start' -%}
        {%- when 'middle' -%}
            {%- assign section_items_alignmentMobile = 'center' -%}
        {%- when 'bottom' -%}
            {%- assign section_items_alignmentMobile = 'flex-end' -%}
      {%- endcase -%}

      #block-{{ section.id }}-{{ block.id }}{

          --section-items-alignment: {{ section_items_alignment }};
          --section-items-alignmentMobile: {{ section_items_alignmentMobile }};

          /* opacity */
          --section-blocks-overlay-color: {{ block.settings.overlay_color.red }}, {{ block.settings.overlay_color.green }}, {{ block.settings.overlay_color.blue }};
          --section-blocks-overlay-opacity: {{ block.settings.overlay_opacity | divided_by: 100.0 }};
          /* text color */
          --text-color: {{ block.settings.text_color.red }}, {{ block.settings.text_color.green }}, {{ block.settings.text_color.blue }};
          /* button color */
          --primary-button-background: {{ block.settings.button_background.red }}, {{ block.settings.button_background.green }}, {{ block.settings.button_background.blue }};
          --primary-button-text-color: {{ block.settings.button_text_color.red }}, {{ block.settings.button_text_color.green }}, {{ block.settings.button_text_color.blue }};

          --font_title:{{block.settings.fontsize_title}}px;
          --font_titleMobile:{{block.settings.fontsize_titleMobile}}px;

          --font_text:{{block.settings.fontsize_text}}px;
          --font_textMobile:{{block.settings.fontsize_textMobile}}px;

      }


  {%- endfor -%}

  .slideshow_all{
      position: relative;
  }
  .slideshow_all .page-width,.slideshowSwiper_box_img  {
    height: 100%;
  }
  .slideshowSwiper{
    overflow: hidden;
  }
  .slideshowSwiper_box {
    height: auto !important;
  }

  .slideshow_all a{
      text-decoration: unset;
  }
  .slideshowSwiper_box img,.slideshowSwiper_box video{
      width: 100%;
      height: 100%;
      object-fit: cover;
  }
  /* box position */
  #shopify-section-{{section.id}} .slideshowSwiper_boxPosition{
      color: rgba(var(--text-color));
      position: absolute;
      flex-direction: column;
      top: 0;
      right: 0;
      left: 0;
      width: 100%;
      height: 100%;
      display: flex;
      content: '';
      background: rgba(var(--section-blocks-overlay-color), var(--section-blocks-overlay-opacity));
  }

  /* donghua */
  .slideshowSwiper_boxPosition{
      transition: all .6s ease;
  }
  .slideshow_all .swiper-slide-active .slideshowSwiper_boxPosition_title{
      transform: translateY(0) translateZ(0);
      opacity: 1;
      transition: opacity .6s linear .1s, transform .6s cubic-bezier(.215,.61,.355,1) .1s;
  }
  .slideshow_all .swiper-slide-active .slideshowSwiper_boxPosition_text{
      transform: translateY(0) translateZ(0);
      opacity: 1;
      transition: opacity .6s linear .2s, transform .6s cubic-bezier(.215,.61,.355,1) .2s;
  }
  .slideshow_all .swiper-slide-active .slideshowSwiper_buttonBox {
      transform: translateY(0) translateZ(0);
      opacity: 1;
      transition: opacity .6s linear .3s, transform .6s cubic-bezier(.215,.61,.355,1) .3s;
  }
  .slideshowSwiper_boxPosition_title,.slideshowSwiper_boxPosition_text,.slideshowSwiper_buttonBox {
      opacity: 0;
      transform: translateY(-60px);
  }
  /*  */


  .slideshowSwiper_boxPositionBox{
    display: flex;
    flex-direction: column;
    height: 100%;
     width: 100%;
  }

  .slideshow_all .swiper-pagination-bullet{
      opacity: 1;
      background: #fff;
      border-radius: 10px;
  }
  .slideshow_all .swiper-button-next::after,.slideshow_all .swiper-button-prev::after{
      content: unset;
  }
  .slideshow_all .swiper-pagination {
      position: absolute;
      height: auto;
      left: 0;
      right: 0;
      bottom: 48px;
      margin: 0;
  }
  .slideshow_all .swiper-pagination span{
      background: rgba(255, 255, 255, 0.5);
      height: 8px;
  }

  .slideshow_all .slideshowSwiper .swiper-pagination-bullet {
      opacity: 1;
      position: relative;
      margin: 0 3px;
      width: 8px;
      height: 4px;
      overflow: hidden;
      transition: all .3s;
      border-radius: 8px;
  }

  .slideshow_all .slideshowSwiper .swiper-pagination-bullet.swiper-pagination-bullet-active{
      width: 20px;
      border-radius: 8px;
      background-color: #fff;
  }

  @keyframes sliderPagination {
    100% {
      transform: scaleX(1);
          opacity: 1;
      }
  }

  @media (max-width:999px) {
      .slideshow_all .swiper-button-prev, .slideshow_all .swiper-button-next{
          display: none;
      }
      .slideshowSwiper_boxPosition{
          padding-top: 96px;
          padding-bottom: 96px;

      }

      .slideshow_all .swiper-pagination-bullet{
          width: 12px;
          height: 12px;
      }
      .slideshow_all .slideshowSwiper .swiper-pagination{
          bottom: 22px;
      }
      .slideshowSwiper_buttonBox{
          display: flex;
          gap: 16px;
          align-items: center;
          margin-top: 16px;
      }
  }

  @media (min-width:1000px) {
     .slideshow_all .swiper-button-prev, .slideshow_all .swiper-button-next  {
        width: 40px;
        height: 40px;
     }
    .slideshow_all .swiper-button-prev .section_button2, .slideshow_all .swiper-button-next .section_button2{
        width: 40px;
        height: 40px;
        border: 1px solid var(--f_color3);
    }
    .slideshow_all .swiper-button-prev svg, .slideshow_all .swiper-button-next svg {
        height: 17px;
        width: auto;
    }
    .slideshow_all .swiper-button-prev:hover .section_button2, .slideshow_all .swiper-button-next:hover .section_button2{
        background-color: #ffffff33;
    }

    .slideshowSwiper_boxPosition{
        padding-top: 96px;
        padding-bottom: 96px;
    }
    .slideshowSwiper_box_img img{
        height:100%;
        min-height:500px;
        object-fit: cover;
    }
    .slideshow_all .slideshowSwiper .swiper-pagination{
        bottom: 40px;
    }
    .slideshowSwiper_boxPositionBoxSmall {
        max-width: 687px;
    }
    .slideshowSwiper_boxPosition_text {
        margin-top: 16px;
    }

    /* 左按钮对齐版心左边 */
    .slideshow_all .swiper-button-prev {
        --page-width: 1540px;
        left: calc((100% - var(--page-width) + 29px) / 2);
    }

    /* 右按钮对齐版心右边 */
    .slideshow_all .swiper-button-next {
        --page-width: 1540px;
        right: calc((100% - var(--page-width) + 29px) / 2);
    }
    .slideshowSwiper_boxPositionBox {
        padding: 0 130px;
    }
  }
  @media (min-width:1000px) and (max-width:1540px) {
    .slideshow_all .swiper-button-prev {
      left: 5rem !important;
    }
    .slideshow_all .swiper-button-next {
      right: 5rem !important;
    }
  }
  @media (max-width:999px) {
    .slideshowSwiper_boxPosition_text {
      margin-top: 12px;
    }
    .slideshow_all .slideshowSwiper .swiper-pagination-bullet{
      height: 2px !important;
    }
  }
</style>
