<div class="slideshow_all" id="shopify-section-{{section.id}}">
  <div class="slideshowSwiper slideshowSwiper_box{{section.settings.section_height}}">
    <div class="swiper-wrapper">
      {% for block in section.blocks %}
        <div class="swiper-slide slideshowSwiper_box" id="block-{{ section.id }}-{{ block.id }}">
          {% if block.settings.video_link != blank %}
            <div class="video-wrapper video-wrapper--native pad_pc" autoplay id="pageAppVideo">
              <!-- controls: true, -->
              {{- block.settings.video_link | video_tag: autoplay: true, playsinline: true, muted: true, loop: true -}}
            </div>
          {% else %}
            <div class="slideshowSwiper_box_img pad_pc">
              <img src="{{block.settings.img | img_url: '1920x'}}" alt="{{block.settings.img.alt}}">
            </div>
          {% endif %}

          {% if block.settings.video_mobileLink != blank %}
            <div class="video-wrapper video-wrapper--native pad_mobile" autoplay id="pageAppVideo">
              <!-- controls: true, -->
              {{-
                block.settings.video_mobileLink
                | video_tag: autoplay: true, playsinline: true, muted: true, loop: true
              -}}
            </div>
          {% else %}
            <div class="slideshowSwiper_box_img pad_mobile">
              <img src="{{block.settings.img_mobile | img_url: '750x'}}" alt="{{block.settings.img_mobile.alt}}">
            </div>
          {% endif %}

          {%- assign text_alignment = block.settings.text_position | split: '_' | first -%}
          {%- assign text_position = block.settings.text_position | split: '_' | last -%}
          {%- assign text_alignmentMobile = block.settings.text_positionMobile | split: '_' | first -%}
          {%- assign text_positionMobile = block.settings.text_positionMobile | split: '_' | last -%}

          {% if block.settings.link != blank %}
            <a href="{{block.settings.link}}" class="slideshowSwiper_boxPosition">
              <div class="page-width">
                <div class="slideshowSwiper_boxPositionBox slideshowSwiper--{{ text_alignment }}--{{ text_position }} slideshowSwiperMobile--{{ text_alignmentMobile }}--{{ text_positionMobile }} area-container">
                  <div class="slideshowSwiper_boxPositionBoxSmall">
                    {% if forloop.first %}
                      {% if block.settings.title != blank %}
                        <h1 class="slideshowSwiper_boxPosition_title font2 my_h_64 my_m_36">
                          {{ block.settings.title }}
                        </h1>
                      {% endif %}
                    {%- else -%}
                      {% if block.settings.title != blank %}
                        <h2 class="slideshowSwiper_boxPosition_title font2 my_h_64 my_m_36">
                          {{ block.settings.title }}
                        </h2>
                      {% endif %}
                    {% endif %}

                    {% if block.settings.text != blank %}
                      <div class="slideshowSwiper_boxPosition_text my_h_16 my_m_14">
                        {{ block.settings.text }}
                      </div>
                    {% endif %}
                  </div>
                </div>
              </div>
            </a>
          {% else %}
            <div class="slideshowSwiper_boxPosition">
              <div class="page-width">
                <div class="slideshowSwiper_boxPositionBox slideshowSwiper--{{ text_alignment }}--{{ text_position }} slideshowSwiperMobile--{{ text_alignmentMobile }}--{{ text_positionMobile }} area-container">
                  <div class="slideshowSwiper_boxPositionBoxSmall">
                    {% if forloop.first %}
                      {% if block.settings.title != blank %}
                        <h1 class="slideshowSwiper_boxPosition_title font2 my_h_64 my_m_36">
                          {{ block.settings.title }}
                        </h1>
                      {% endif %}
                    {%- else -%}
                      {% if block.settings.title != blank %}
                        <h2 class="slideshowSwiper_boxPosition_title font2 my_h_64 my_m_36">
                          {{ block.settings.title }}
                        </h2>
                      {% endif %}
                    {% endif %}
                    {% if block.settings.text != blank %}
                      <div class="slideshowSwiper_boxPosition_text my_h_16 my_m_14">
                        {{ block.settings.text }}
                      </div>
                    {% endif %}
                  </div>
                </div>
              </div>
            </div>
          {% endif %}
        </div>
      {% endfor %}
    </div>
    {% if section.blocks.size > 1 %}
      <div class="swiper-button-prev">
        <div class="section_button2">
          <svg xmlns="http://www.w3.org/2000/svg" width="10" height="18" viewBox="0 0 10 18" fill="none">
            <path d="M8.625 17.125L9.875 15.875L2.50001 8.56249L9.875 1.25L8.625 0L7.62939e-06 8.56249L8.625 17.125Z" fill="white"/>
          </svg>
          <span class="corner-tl"></span>
          <span class="corner-br"></span>
        </div>
      </div>
      <div class="swiper-pagination"></div>
      <div class="swiper-button-next">
        <div class="section_button2">
          <svg xmlns="http://www.w3.org/2000/svg" width="10" height="18" viewBox="0 0 10 18" fill="none">
            <path d="M1.25 17.125L0 15.875L7.37499 8.56249L0 1.25L1.25 0L9.87499 8.56249L1.25 17.125Z" fill="white"/>
          </svg>
          <span class="corner-tl"></span>
          <span class="corner-br"></span>
        </div>
      </div>
    {% endif %}
  </div>
</div>

<script>
  var swiperItems = document.querySelectorAll('.slideshow_all .swiper-slide');
  var autoplayDelay = {{section.settings.time}} * 1000;

  var enableLoop = swiperItems.length > 1; // 检查项目数量是否大于1
  console.log(enableLoop);

  var mySwiper;

  if (autoplayDelay > 0) {
      mySwiper = new Swiper('.slideshowSwiper', {
          speed: 400,
          slidesPerView: 1,
          loop: enableLoop,

          observer: true,  //开启动态检查器，监测swiper和slide
          observeParents: true,  //监测Swiper 的祖/父元素
          navigation: {
              nextEl: ".slideshow_all .swiper-button-next",
              prevEl: ".slideshow_all .swiper-button-prev",
          },
          // 添加页面样式
          pagination: {
              el: '.swiper-pagination', // 要识别为页面字符的类
              clickable: true, //单击“页面”按钮后移动
          },
          on: {
               slideChange: function() {
                    if (this.realIndex > 0) {
                        this.params.autoplay.delay = autoplayDelay;
                    }
                }
          },
          autoplay: {
              disableOnInteraction: false,
              delay: autoplayDelay,
          }
      });
  } else {
      mySwiper = new Swiper('.slideshowSwiper', {
          speed: 400,
          slidesPerView: 1,
          loop: false,
          // allowTouchMove: false,
          observer: true,
          observeParents: true,
          navigation: {
              nextEl: ".slideshow_all .swiper-button-next",
              prevEl: ".slideshow_all .swiper-button-prev",
          },
          // 添加页面样式
          pagination: {
              el: '.swiper-pagination',
              clickable: true,
          },
          on: {
              slideChange: function() {
                  if (this.realIndex > 0) {
                      this.params.autoplay.delay = autoplayDelay;
                  }
              }
          },
          autoplay: false // Disable autoplay
      });
  }
</script>
{% schema %}
{
  "name": "index- Slideshow",
  "class": "shopify-section--index-slideshow",
  "settings": [
    {
      "type": "range",
      "id": "time",
      "label": "time",
      "unit": "s",
      "min": 0,
      "max": 10,
      "step": 1,
      "default": 5
    },
    {
      "type": "header",
      "content": "Spacing Top:"
    },
    {
      "type": "number",
      "id": "Desktop_top",
      "label": "Desktop Margin Top",
      "default": 0
    },
    {
      "type": "number",
      "id": "Mobile_top",
      "label": "Mobile Margin Top",
      "default": 0
    },
    {
      "type": "header",
      "content": "Spacing Bottom:"
    },
    {
      "type": "number",
      "id": "Desktop_bottom",
      "label": "Desktop Margin Bottom",
      "default": 0
    },
    {
      "type": "number",
      "id": "Mobile_bottom",
      "label": "Mobile Margin Bottom",
      "default": 0
    }
  ],
  "blocks": [
    {
      "type": "image",
      "name": "Image",
      "settings": [
        {
          "type": "image_picker",
          "id": "img",
          "info": "2160 x 1080px .jpg recommended, 1080 x 1080px .jpg recommended if split",
          "label": "Image"
        },

        {
          "type": "image_picker",
          "id": "img_mobile",
          "label": "Mobile image",
          "info": "1000 x 1400px .jpg recommended. If none is set, desktop image will be used."
        },
        {
          "type": "video",
          "id": "video_link",
          "label": "video link"
        },
        {
