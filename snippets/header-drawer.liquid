<script>
  // tab top

  $('.header_menu').click(function () {
    $('.menu-drawer').addClass('active');
    $('body').addClass('overflow-hidden');
  });

  /* 点击遮罩关闭 */
  $('.menu-drawer').on('click', function () {
    $(this).removeClass('active');
    $('body').removeClass('overflow-hidden');
  });

  /* 内容区域不关闭 */
  $('.drawer__content').on('click', function (e) {
    e.stopPropagation();
  });

  /* 你已有的关闭按钮 */
  $('.drawer__contenClose').click(function (e) {
    e.stopPropagation(); // 可选，防止冒泡
    $('body').removeClass('overflow-hidden');
    $(this).parents('.menu-drawer').removeClass('active');
  });

  /* 其他逻辑保持不变 */
  $('.desktop_headerRightTop .mobile_close').click(function (event) {
    event.stopPropagation();
    $(this).parents('.mobile_nav__itemBox1').removeClass('active');
  });

  $('.mobile_navMenuList_itemTitle').click(function () {
    $(this).next().addClass('active');
  });

  $('.mega_menu_leftBottom2_listTopM').click(function () {
    $(this).next('.mega_menu_leftBottom2_bottom').slideToggle();
    $(this).toggleClass('mega_menu_leftBottom2_listTopMs');
